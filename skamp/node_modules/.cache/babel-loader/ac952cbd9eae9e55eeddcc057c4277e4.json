{"ast":null,"code":"import store from \"@/store.js\";\nexport default {\n  data() {\n    return {\n      crntLang: \"hr\",\n      currentYear: new Date().getFullYear(),\n      langs: [\"Hrvatski\", \"Engleski\", \"Njemački\", \"Talijanski\"],\n      textHR: {\n        1: \"Početna\",\n        2: \"O nama\",\n        3: \"Limarija\",\n        4: \"Strojna obrada\",\n        5: \"Bravarija\",\n        6: \"Galerija\",\n        7: \"Kontakt\"\n      }\n    };\n  },\n\n  computed: {\n    textUpdate(lang, id) {\n      let returnWord = \"\";\n\n      switch (lang) {\n        case \"hr\":\n          {\n            for (const [key, value] of Object.entries(this.textHR)) {\n              if (key == id) returnWord = `${value}`;\n            }\n          }\n      }\n\n      return returnWord;\n    }\n\n  },\n  methods: {\n    changeLanguage(crntLang) {\n      store.lang = crntLang;\n      this.crntLang = store.lang;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkLA,OAAOA,KAAP,MAAkB,YAAlB;AACA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,IADL;AAELC,iBAAW,EAAE,IAAIC,IAAJ,GAAWC,WAAX,EAFR;AAGLC,WAAK,EAAE,CAAC,UAAD,EAAa,UAAb,EAAyB,UAAzB,EAAqC,YAArC,CAHF;AAILC,YAAM,EAAE;AACN,WAAG,SADG;AAEN,WAAG,QAFG;AAGN,WAAG,UAHG;AAIN,WAAG,gBAJG;AAKN,WAAG,WALG;AAMN,WAAG,UANG;AAON,WAAG;AAPG;AAJH,KAAP;AAcD,GAhBY;;AAiBbC,UAAQ,EAAE;AACRC,cAAU,CAACC,IAAD,EAAOC,EAAP,EAAW;AACnB,UAAIC,UAAS,GAAI,EAAjB;;AACA,cAAQF,IAAR;AACE,aAAK,IAAL;AAAW;AACT,iBAAK,MAAM,CAACG,GAAD,EAAMC,KAAN,CAAX,IAA2BC,MAAM,CAACC,OAAP,CAAe,KAAKT,MAApB,CAA3B,EAAwD;AACtD,kBAAIM,GAAE,IAAKF,EAAX,EAAeC,UAAS,GAAK,GAAEE,KAAM,EAAtB;AACjB;AACF;AALF;;AAOA,aAAOF,UAAP;AACD;;AAXO,GAjBG;AA8BbK,SAAO,EAAE;AACPC,kBAAc,CAAChB,QAAD,EAAW;AACvBF,WAAK,CAACU,IAAN,GAAaR,QAAb;AACA,WAAKA,QAAL,GAAgBF,KAAK,CAACU,IAAtB;AACD;;AAJM;AA9BI,CAAf","names":["store","data","crntLang","currentYear","Date","getFullYear","langs","textHR","computed","textUpdate","lang","id","returnWord","key","value","Object","entries","methods","changeLanguage"],"sources":["C:\\Users\\Lu
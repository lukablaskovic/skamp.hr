  class=\"flex mt-8 p-8 blackBackground text-gray-200 mx-auto place-content-center\"\n    >\n      <div class=\"w-full overflow-hidden xl:w-1/3 text-center\">\n        <h3>\n          <b>Copyright © {{ currentYear }} Škamp d.o.o.</b>\n        </h3>\n        <h2>Rudarska ulica 12, Vodnjan</h2>\n        <h2>+385 98 214 349</h2>\n        <h2>skamp.pu@gmail.com</h2>\n      </div>\n    </div>\n    <!--/FOOTER-->\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport ButtonOrange from \"../components/ButtonOrange.vue\";\nimport { GoogleMap, Marker } from \"vue3-google-map\";\n\nexport default {\n  name: \"HomeView\",\n  data() {\n    return {\n      currentYear: new Date().getFullYear() + \".\",\n    };\n  },\n  components: {\n    ButtonOrange,\n    GoogleMap,\n    Marker,\n  },\n  setup() {\n    const center = { lat: 44.96177, lng: 13.86066 };\n\n    return { center };\n  },\n};\n</script>\n<style scoped>\n.align-icon-middle {\n  display: flex;\n  align-items: center;\n}\n.footerText {\n  opacity: 1 !important;\n}\n\n.align-icon-middle span {\n  padding-left: 5px;\n}\n\n/* Container holding the image and the text */\n.containerC {\n  position: relative;\n  color: #dbdbdb;\n}\n\n/* Top left text */\n.top-left {\n  position: absolute;\n  top: 80px;\n  left: 100px;\n  text-align: left;\n}\n.top-left-mobile {\n  position: absolute;\n  top: 35px;\n  left: 95px;\n  text-align: left;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}
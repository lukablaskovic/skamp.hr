n      <!----------LOCATION--------->\n      <!--Google maps-->\n      <div class=\"overflow-hidden mt-4 md:mt-8 ml-24 mr-24\">\n        <GoogleMap\n          api-key=\"AIzaSyC1bGd4Skv5XVAhFpB9IuIovNRAQR0rogk\"\n          style=\"width: 100%; height: 500px\"\n          :center=\"center\"\n          :zoom=\"14\"\n        >\n          <Marker :options=\"{ position: center }\" />\n        </GoogleMap>\n      </div>\n      <!--/Google maps-->\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport ButtonOrange from \"../components/ButtonOrange.vue\";\nimport { GoogleMap, Marker } from \"vue3-google-map\";\n\nexport default {\n  name: \"HomeView\",\n\n  components: {\n    ButtonOrange,\n    GoogleMap,\n    Marker,\n  },\n  setup() {\n    const center = { lat: 44.96177, lng: 13.86066 };\n\n    return { center };\n  },\n};\n</script>\n<style scoped>\n.align-icon-middle {\n  display: flex;\n  align-items: center;\n}\n.footerText {\n  opacity: 1 !important;\n}\n\n.align-icon-middle span {\n  padding-left: 5px;\n}\n\n/* Container holding the image and the text */\n.containerC {\n  position: relative;\n  color: #dbdbdb;\n}\n\n/* Top left text */\n.top-left {\n  position: absolute;\n  top: 100px;\n  left: 160px;\n  text-align: left;\n}\n.top-left-mobile {\n  position: absolute;\n  top: 35px;\n  left: 95px;\n  text-align: left;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}
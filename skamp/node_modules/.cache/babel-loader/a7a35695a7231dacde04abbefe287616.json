{"ast":null,"code":"// @ is an alias to /src\nimport { GoogleMap, Marker } from \"vue3-google-map\";\nimport emailjs from \"@emailjs/browser\";\nexport default {\n  name: \"HomeView\",\n\n  data() {\n    return {\n      ime: \"\",\n      prezime: \"\",\n      poruka: \"\",\n      email: \"\"\n    };\n  },\n\n  components: {\n    GoogleMap,\n    Marker\n  },\n\n  setup() {\n    const center = {\n      lat: 44.96177,\n      lng: 13.86066\n    };\n    return {\n      center\n    };\n  },\n\n  methods: {\n    sendEmail() {\n      if (this.ime == \"\" || this.prezime == \"\" || this.poruka == \"\" || this.email == \"\") {\n        alert(\"Molimo unesite sve podatke\");\n        return;\n      }\n\n      emailjs.send(\"service_w58icuj\", \"template_fy73qcl\", {\n        from_name: this.ime + \" \" + this.prezime,\n        from_email: this.email,\n        message: this.poruka\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6GA;AACA,SAASA,SAAT,EAAoBC,MAApB,QAAkC,iBAAlC;AACA,OAAOC,OAAP,MAAoB,kBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,SAAG,EAAE,EADA;AAELC,aAAO,EAAE,EAFJ;AAGLC,YAAM,EAAE,EAHH;AAILC,WAAK,EAAE;AAJF,KAAP;AAMD,GATY;;AAUbC,YAAU,EAAE;AACVT,aADU;AAEVC;AAFU,GAVC;;AAcbS,OAAK,GAAG;AACN,UAAMC,MAAK,GAAI;AAAEC,SAAG,EAAE,QAAP;AAAiBC,SAAG,EAAE;AAAtB,KAAf;AAEA,WAAO;AAAEF;AAAF,KAAP;AACD,GAlBY;;AAmBbG,SAAO,EAAE;AACPC,aAAS,GAAG;AACV,UACE,KAAKV,GAAL,IAAY,EAAZ,IACA,KAAKC,OAAL,IAAgB,EADhB,IAEA,KAAKC,MAAL,IAAe,EAFf,IAGA,KAAKC,KAAL,IAAc,EAJhB,EAKE;AACAQ,aAAK,CAAC,4BAAD,CAAL;AACA;AACF;;AACAd,aAAO,CAACe,IAAR,CAAa,iBAAb,EAAgC,kBAAhC,EAAoD;AAClDC,iBAAS,EAAE,KAAKb,GAAL,GAAW,GAAX,GAAiB,KAAKC,OADiB;AAElDa,kBAAU,EAAE,KAAKX,KAFiC;AAGlDY,eAAO,EAAE,KAAKb;AAHoC,OAApD;AAKD;;AAhBM;AAnBI,CAAf","names":["GoogleMap","Marker","emailjs","name","data","ime","prezime","poruka","email","components","setup","center","lat","lng","methods","sendEmail","alert","send","from_name","from_email","message"],"sources":["C:\\Users\\Luka\\Desktop\\skamp\\src\\views\\KontaktView.vue"],"sourcesContent":["<template>\n  <!----------TEXT-IMAGE (2)--------->\n  <div\n    class=\"flex flex-col md:flex-row overflow-hidden blackBackground md:mr-64 md:ml-64\"\n  >\n    <!--KONTAKT FORMA-->\n    <div class=\"w-full overflow-hidden text-left mt-4 ml-6\">\n      <h1 class=\"text-3xl md:text-4xl text-yellow-500 mb-4\">\n        <b>KONTAKTIRAJTE NAS</b\n        ><!--<span class=\"text-white\"> (u izradi...)</span>-->\n      </h1>\n      <form class=\"md:w-1/2 mr-12\">\n        <div class=\"flex flex-wrap -mx-3 mb-6\">\n          <div class=\"w-full md:w-1/2 px-3 mb-6 md:mb-0\">\n            <label\n              class=\"block uppercase tracking-wide text-yellow-500 text-xs font-bold mb-2\"\n              for=\"grid-first-name\"\n            >\n              Ime\n            </label>\n            <input\n              class=\"appearance-none block w-full bg-gray-200 text-black border border-gray-500 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white\"\n              id=\"grid-first-name\"\n              type=\"text\"\n              placeholder=\"Unesite vaše ime\"\n              v-model=\"ime\"\n            />\n            <!--<p class=\"text-red-500 text-xs italic\">\n              Molimo ispunite ovo polje.\n            </p>-->\n          </div>\n          <div class=\"w-full md:w-1/2 px-3\">\n            <label\n              class=\"block uppercase tracking-wide text-yellow-500 text-xs font-bold mb-2\"\n              for=\"grid-last-name\"\n            >\n              Prezime\n            </label>\n            <input\n              class=\"appearance-none block w-full bg-gray-200 text-black border border-gray-500 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500\"\n              id=\"grid-last-name\"\n              type=\"text\"\n              placeholder=\"Prezime\"\n              v-model=\"prezime\"\n            />\n          </div>\n        </div>\n        <div class=\"flex flex-wrap -mx-3 mb-6\">\n          <div class=\"w-full px-3\">\n            <label\n              class=\"block uppercase tracking-wide text-yellow-500 text-xs font-bold mb-2\"\n              for=\"grid-password\"\n            >\n              E-mail adresa\n            </label>\n            <input\n              class=\"appearance-none block w-full bg-gray-200 text-black border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white focus:border-gray-500\"\n              id=\"email\"\n              type=\"email\"\n              v-model=\"email\"\n            />\n          </div>\n        </div>\n        <div class=\"flex flex-wrap -mx-3 mb-6\">\n          <div class=\"w-full px-3\">\n            <label\n              class=\"block uppercase tracking-wide text-yellow-500 text-xs font-bold mb-2\"\n              for=\"grid-password\"\n            >\n              Poruka\n            </label>\n            <textarea\n              class=\"no-resize appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 h-48 resize-none\"\n              id=\"message\"\n              v-model=\"poruka\"\n            ></textarea>\n          </div>\n        </div>\n        <div class=\"md:flex md:items-center text-center md:text-left\">\n          <div class=\"md:w-1/3\">\n            <button\n              class=\"shadow bg-yellow-500 hover:bg-yellow-600 focus:shadow-outline focus:outline-none text-black font-bold py-2 px-4 rounded mb-4\"\n              type=\"button\"\n              @click.prevent=\"sendEmail\"\n            >\n              Pošalji\n            </button>\n          </div>\n          <div class=\"md:w-2/3\"></div>\n        </div>\n      </form>\n    </div>\n    <!--/KONTAKT FORMA-->\n    <!--Google maps-->\n    <div class=\"overflow-hidden w-full md:w-1/2 md:mr-16 md:mt-24\">\n      <GoogleMap\n        api-key=\"AIzaSyCAx1d5yyFL-_3NX2D12OUMgwQ7XMZtEr4\"\n        style=\"width: 100%; height: 400px\"\n        :center=\"center\"\n        :zoom=\"14\"\n      >\n        <Marker :options=\"{ position: center }\" />\n      </GoogleMap>\n    </div>\n    <!--/Google maps-->\n  </div>\n  <!----------/TEXT-IMAGE (2)--------->\n</template>\n<script>\n// @ is an alias to /src\nimport { GoogleMap, Marker } from \"vue3-google-map\";\nimport emailjs from \"@emailjs/browser\";\n\nexport default {\n  name: \"HomeView\",\n  data() {\n    return {\n      ime: \"\",\n      prezime: \"\",\n      poruka: \"\",\n      email: \"\",\n    };\n  },\n  components: {\n    GoogleMap,\n    Marker,\n  },\n  setup() {\n    const center = { lat: 44.96177, lng: 13.86066 };\n\n    return { center };\n  },\n  methods: {\n    sendEmail() {\n      if (\n        this.ime == \"\" ||\n        this.prezime == \"\" ||\n        this.poruka == \"\" ||\n        this.email == \"\"\n      ) {\n        alert(\"Molimo unesite sve podatke\");\n        return;\n      }\n      emailjs.send(\"service_w58icuj\", \"template_fy73qcl\", {\n        from_name: this.ime + \" \" + this.prezime,\n        from_email: this.email,\n        message: this.poruka,\n      });\n    },\n  },\n};\n</script>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}
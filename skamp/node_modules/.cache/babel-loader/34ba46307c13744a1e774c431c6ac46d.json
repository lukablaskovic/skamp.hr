{"ast":null,"code":"// @ is an alias to /src\nimport ButtonOrange from \"../components/ButtonOrange.vue\";\nimport { GoogleMap, Marker } from \"vue3-google-map\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    ButtonOrange,\n    GoogleMap,\n    Marker\n  },\n\n  setup() {\n    const center = {\n      lat: 44.96177,\n      lng: 13.86066\n    };\n    return {\n      center\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuIA;AACA,OAAOA,YAAP,MAAyB,gCAAzB;AACA,SAASC,SAAT,EAAoBC,MAApB,QAAkC,iBAAlC;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVJ,gBADU;AAEVC,aAFU;AAGVC;AAHU,GAFC;;AAObG,OAAK,GAAG;AACN,UAAMC,MAAK,GAAI;AAAEC,SAAG,EAAE,QAAP;AAAiBC,SAAG,EAAE;AAAtB,KAAf;AAEA,WAAO;AAAEF;AAAF,KAAP;AACD;;AAXY,CAAf","names":["ButtonOrange","GoogleMap","Marker","name","components","setup","center","lat","lng"],"sources":["C:\\Users\\Luka\\Desktop\\skamp\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <!----------TEXT-IMAGE (2)--------->\n    <div class=\"flex flex-wrap overflow-hidden\">\n      <div class=\"w-full overflow-hidden xl:w-1/2 text-center\">\n        <h1 class=\"text-3xl xl:text-7xl text-yellow-600 mt-6 xl:mt-64\">\n          PROIZVODNJA ČELIČNIH KONSTRUKCIJA\n        </h1>\n        <h2 class=\"text-2xl xl:text-4xl text-yellow-800\">\n          Vama na usluzi od 2012. godine!\n        </h2>\n        <ButtonOrange btnText=\"O nama\" btnLink=\"o-nama\" />\n      </div>\n\n      <div class=\"w-full overflow-hidden xl:w-1/2\">\n        <img src=\"@/assets/pocetna.jpg\" alt=\"\" />\n      </div>\n    </div>\n    <!----------/TEXT-IMAGE (2)--------->\n\n    <!----------Kartice--------->\n    <div class=\"flex flex-wrap overflow-hidden\">\n      <div class=\"w-full overflow-hidden xl:w-1/3 text-center\">\n        <img\n          class=\"h-2/3 w-full animate__animated animate__zoomIn xl:p-12\"\n          src=\"@/assets/limarija.jpg\"\n          alt=\"\"\n        />\n        <ButtonOrange btnText=\"Limarija\" btnLink=\"limarija\" />\n      </div>\n\n      <div class=\"w-full overflow-hidden xl:w-1/3 text-center\">\n        <img\n          class=\"h-2/3 w-full animate__animated animate__zoomIn xl:p-12\"\n          src=\"@/assets/strojna.jpg\"\n          alt=\"\"\n        />\n        <ButtonOrange btnText=\"Strojna obrada\" btnLink=\"strojna-obrada\" />\n      </div>\n\n      <div class=\"w-full overflow-hidden xl:w-1/3 text-center\">\n        <img\n          class=\"h-2/3 w-full animate__animated animate__zoomIn xl:p-12\"\n          src=\"@/assets/bravarija.jpg\"\n          alt=\"\"\n        />\n        <ButtonOrange btnText=\"Bravarija\" btnLink=\"bravarija\" />\n      </div>\n    </div>\n    <!----------/Kartice--------->\n    <!----------4 TEXTS--------->\n    <div class=\"flex flex-wrap overflow-hidden ml-64 mr-64\">\n      <div class=\"w-full overflow-hidden xl:w-1/2\">\n        <h2 class=\"text-7xl\">\n          Izrada metalnih konstrukcija, prerada i obrada metala\n        </h2>\n      </div>\n\n      <div class=\"w-full overflow-hidden xl:w-1/2\">\n        <h2 class=\"\">\n          Pogon u Vodnjanu specijalizira u obradu metala, limariju i bravariju.\n        </h2>\n        <h2>\n          <b>Škamp d.o.o</b> u sklopu svog pogona može izrađivati metalne\n          konstrukcije malih i velikih dimenzija do maximun 20 tona.\n          Zapošljavamo vrhunsku bravere i zavarivače te u sklopu bravarija imamo\n          sve potrebne strojeve (pile, CNC laser, CNC plazma, CNC savijačice s 4\n          osi).\n        </h2>\n      </div>\n      <div></div>\n      <div class=\"w-full overflow-hidden xl:w-1/2 mt-16 mb-16\">\n        <h2 class=\"text-6xl\">\n          Proširenje radnih prostora prema Europskom standardu\n        </h2>\n      </div>\n\n      <div class=\"w-full overflow-hidden xl:w-1/2 mt-16 mb-16\">\n        <h2 class=\"\">\n          U skladu sa vremenom pogon se stalno modernizira te je konkurentan na\n          tržištu i prilagodljiv današnjim situacijama i različitim zahtjevima.\n        </h2>\n        <h2>\n          Uz <b>tehnološku modernizaciju</b> napravljeno je puno na proširenju\n          radnih/poslovnih prostora koji odgovaraju europskim uvjetima. Pogon\n          zapošljava 15 djelatnika koji su stručni spremni i usposobljeni za sve\n          navedene poslove u preradi i obradi metala.\n        </h2>\n        <h2>\n          Škamp d.o.o posluje sa hrvatskim, talijanskim i njemačkim tvrtkama.\n          Naši proizvodi zadovoljavaju europsko tržište.\n        </h2>\n      </div>\n    </div>\n    <!----------/4 TEXTS--------->\n    <div\n      class=\"flex flex-col overflow-hidden bg-yellow-600 p-6 text-center text-4xl ml-32 mr-32\"\n    >\n      NAŠA LOKACIJA\n      <span>Rudarska ulica 12, Vodnjan</span>\n    </div>\n    <!--Google maps-->\n    <div class=\"flex flex-col overflow-hidden ml-32 mr-32\">\n      <GoogleMap\n        api-key=\"AIzaSyC1bGd4Skv5XVAhFpB9IuIovNRAQR0rogk\"\n        style=\"width: 100%; height: 500px\"\n        :center=\"center\"\n        :zoom=\"14\"\n      >\n        <Marker :options=\"{ position: center }\" />\n      </GoogleMap>\n    </div>\n    <!--/FOOTER-->\n    <div class=\"mt-16 bg-radiona-footer bg-fixed p-48 bg-cover\">\n      <div class=\"flex flex-wrap overflow-hidden\">\n        <div class=\"w-full overflow-hidden xl:w-1/3 text-center\">\n          <h3 class=\"text-2xl\"><b>Adresa</b></h3>\n          <h2>Rudarska ulica 12</h2>\n          <h2>Vodnjan</h2>\n        </div>\n\n        <div class=\"w-full overflow-hidden xl:w-1/3 text-center\">\n          <h3 class=\"text-2xl\"><b>Kontakt</b></h3>\n        </div>\n\n        <div class=\"w-full overflow-hidden xl:w-1/3 text-center\">\n          <h3 class=\"text-2xl\"><b>Dodatne info...</b></h3>\n        </div>\n      </div>\n    </div>\n    <!--/FOOTER-->\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport ButtonOrange from \"../components/ButtonOrange.vue\";\nimport { GoogleMap, Marker } from \"vue3-google-map\";\n\nexport default {\n  name: \"HomeView\",\n  components: {\n    ButtonOrange,\n    GoogleMap,\n    Marker,\n  },\n  setup() {\n    const center = { lat: 44.96177, lng: 13.86066 };\n\n    return { center };\n  },\n};\n</script>\n<style scoped>\n.align-icon-middle {\n  display: flex;\n  align-items: center;\n}\n.footerText {\n  opacity: 1 !important;\n}\n\n.align-icon-middle span {\n  padding-left: 5px;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}